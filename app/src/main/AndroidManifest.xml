            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Procedures_act"
            android:label="@string/title_activity_procedures_act" >
        </activity>
        <activity
            android:name=".Activity4Step"
            android:label="@string/title_activity_steps_" >
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/title_activity_main" >
        </activity>
    </application>

</manifest>
